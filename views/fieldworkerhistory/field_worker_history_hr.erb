      <% end %>
      <% if field_worker_history_table[0].has_key?('currentCivilServant') %>
        <th>Current Civil Servant</th>
      <% end %>
      <% if field_worker_history_table[0].has_key?('previousCivilServant') %>
        <th>Previous Civil Servant</th>
      <% end %>
    </tr>
    </thead>
    <tbody class="primary">
    <% field_worker_history_table.each_with_index do |field_worker_history| %>
      <tr>
        <% if field_worker_history['ingestDate'].present? %>
          <td><%= h field_worker_history['ingestDate'] %></td>
