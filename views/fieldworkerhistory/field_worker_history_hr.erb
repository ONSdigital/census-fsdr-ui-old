        <% else %>
          <td>-</td>
        <% end %>
        <% if field_worker_history['civilServicePensionRecipient'].present? %>
          <% if field_worker_history['civilServicePensionRecipient'] == true %>
          <td>Yes</td>
          <% else %>
          <td>No</td>
          <% end %>
        <% else %>
          <td>-</td>
        <% end %>
        <% if field_worker_history['currentCivilServant'].present? %>
          <% if field_worker_history['currentCivilServant'] == true %>
            <td>Yes</td>
          <% else %>
            <td>No</td>
          <% end %>
        <% else %>
          <td>-</td>
        <% end %>
        <% if field_worker_history['previousCivilServant'].present? %>
          <% if field_worker_history['previousCivilServant'] == true %>
            <td>Yes</td>
          <% else %>
            <td>No</td>
          <% end %>
        <% else %>
          <td>-</td>
        <% end %>
      </tr>
    <% end %>
